<?xml version="1.0" encoding="UTF-8"?>

<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.5.5</version>
        <relativePath/>
    </parent>

    <groupId>com.zerodeplibs</groupId>
    <artifactId>zerodep-web-push-java-example</artifactId>
    <version>1.0.1</version>

    <name>zerodep-web-push-java-example</name>
    <url>https://github.com/st-user/zerodep-web-push-java-example</url>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <!--
            JDK8+ is required.
        -->
        <java.version>8</java.version>
        <zerodeplibs.version>1.1.0-SNAPSHOT</zerodeplibs.version>
    </properties>

    <dependencies>

        <dependency>
            <groupId>com.zerodeplibs</groupId>
            <artifactId>zerodep-web-push-java</artifactId>
            <version>${zerodeplibs.version}</version>
        </dependency>

        <!--
            When using [Java JWT - auth0](https://github.com/auth0/java-jwt).
        -->
        <dependency>
            <groupId>com.zerodeplibs</groupId>
            <artifactId>zerodep-web-push-java-ext-jwt-auth0</artifactId>
            <version>${zerodeplibs.version}</version>
            <scope>runtime</scope>
        </dependency>
        <!--
            If you want to explicitly specify the version:-->
            <dependency>
                <groupId>com.auth0</groupId>
                <artifactId>java-jwt</artifactId>
                <!--<version>${your.auth0.version}</version>-->
                <version>3.17.0</version>
                <scope>runtime</scope>
            </dependency>





        <!--
            When using [FusionAuth JWT](https://github.com/fusionauth/fusionauth-jwt).

            <dependency>
                <groupId>com.zerodeplibs</groupId>
                <artifactId>zerodep-web-push-java-ext-jwt-fusionauth</artifactId>
                <version>${zerodeplibs.version}</version>
                <scope>runtime</scope>
            </dependency>

            If you want to explicitly specify the version:
            <dependency>
                <groupId>io.fusionauth</groupId>
                <artifactId>fusionauth-jwt</artifactId>
                <version>${your.fusionauth.version}</version>
                <scope>runtime</scope>
            </dependency>
        -->

        <!--
            When using [Java JWT](https://github.com/jwtk/jjwt).

            <dependency>
                <groupId>com.zerodeplibs</groupId>
                <artifactId>zerodep-web-push-java-ext-jwt-jjwt</artifactId>
                <version>${zerodeplibs.version}</version>
                <scope>runtime</scope>
            </dependency>

            If you want to explicitly specify the version:
            <dependency>
                <groupId>io.jsonwebtoken</groupId>
                <artifactId>jjwt-api</artifactId>
                <version>${your.jjwt.version}</version>
                <scope>runtime</scope>
            </dependency>

            Java JWT's runtime dependencies.
            (see https://github.com/jwtk/jjwt#jdk-projects).
            <dependency>
                <groupId>io.jsonwebtoken</groupId>
                <artifactId>jjwt-impl</artifactId>
                <version>${your.jjwt.version}</version>
                <scope>runtime</scope>
            </dependency>
            <dependency>
                <groupId>io.jsonwebtoken</groupId>
                <artifactId>jjwt-jackson</artifactId> or jjwt-gson if Gson is preferred
                <version>${your.jjwt.version}</version>
                <scope>runtime</scope>
            </dependency>
        -->

        <!--
            When using [jose4j](https://bitbucket.org/b_c/jose4j/wiki/Home).

            <dependency>
                <groupId>com.zerodeplibs</groupId>
                <artifactId>zerodep-web-push-java-ext-jwt-jose4j</artifactId>
                <version>${zerodeplibs.version}</version>
                <scope>runtime</scope>
            </dependency>

            If you want to explicitly specify the version:
            <dependency>
                <groupId>org.bitbucket.b_c</groupId>
                <artifactId>jose4j</artifactId>
                <version>${your.jose4j.version}</version>
                <scope>runtime</scope>
            </dependency>
        -->

        <!--
            [Nimbus JOSE + JWT](https://connect2id.com/products/nimbus-jose-jwt).

            <dependency>
                <groupId>com.zerodeplibs</groupId>
                <artifactId>zerodep-web-push-java-ext-jwt-nimbus-jose</artifactId>
                <version>${zerodeplibs.version}</version>
                <scope>runtime</scope>
            </dependency>

            If you want to explicitly specify the version:

            <dependency>
                <groupId>com.nimbusds</groupId>
                <artifactId>nimbus-jose-jwt</artifactId>
                <version>${your.nimbus.jose.version}</version>
                <scope>runtime</scope>
            </dependency>
        -->


        <!--
            This project utilizes OkHttp.
            Of course, you can use any http client library in your project.
        -->
        <dependency>
            <groupId>com.squareup.okhttp3</groupId>
            <artifactId>okhttp</artifactId>
            <version>4.9.0</version>
        </dependency>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
        </dependency>

    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
            </plugin>
        </plugins>
    </build>
</project>
